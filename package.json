{
  "name": "smi-jira-alert-integration",
  "version": "2.0.0",
  "description": "A simple Alert conversion and data fetch flow from Azure to Jira",
  "author": "Jacob Connell",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "script": "ts-node -r tsconfig-paths/register",
    "typecheck": "tsc --p tsconfig.json --noEmit",
    "format": "prettier --write **/*.{ts,js,json}",
    "format:check": "prettier --check **/*.{ts,js,json}",
    "start:host": "func start",
    "start": "concurrently --names \"func,tsc\" -c \"auto\" -k \"npm run start:host\" \"npm run watch\" ",
    "dev": "npm run build && npm run start",
    "build:production": "npm run build && npm prune --production",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "checks": "npm run lint && npm run format:check && npm run test && npm run build"
  },
  "dependencies": {
    "@atlaskit/adf-utils": "^19.3.0",
    "@qcyber/azure": "^2.0.22",
    "axios": "^1.7.2",
    "azure-functions-core-tools": "^4.0.5907",
    "jira-client": "^8.2.2",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@azure/functions": "^3.5.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jira-client": "^7.1.9",
    "@types/node": "^20.11.21",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "func": "^1.3.5",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  }
}
